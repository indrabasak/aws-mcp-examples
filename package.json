{
  "name": "aws-mcp-examples",
  "version": "1.0.0",
  "description": "AWS MCP Examples - TypeScript Serverless Application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "deploy": "BUILDX_NO_DEFAULT_ATTESTATIONS=1 serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "test": "jest",
    "test:watch": "jest --watch",
    "local": "serverless offline",
    "start:express": "tsc && node dist/src/server.js",
    "dev:express": "tsc -w & node dist/src/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.145",
    "aws-lambda": "^1.0.7",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "typescript",
    "mcp",
    "api-gateway"
  ],
  "author": "",
  "license": "MIT"
}
