{
  "name": "aws-mcp-examples",
  "version": "1.0.0",
  "type": "module",
  "description": "AWS MCP Examples - TypeScript Serverless Application",
  "author": "Indra Basak",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "deploy": "BUILDX_NO_DEFAULT_ATTESTATIONS=1 serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "test": "jest",
    "test:watch": "jest --watch",
    "local": "serverless offline",
    "start:express": "tsc && node dist/src/server.js",
    "dev:express": "tsc -w & node dist/src/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@azure/identity": "^4.10.1",
    "@langchain/core": "^0.3.58",
    "@langchain/langgraph": "^0.3.2",
    "@langchain/mcp-adapters": "^0.5.2",
    "@langchain/openai": "^0.5.13",
    "@modelcontextprotocol/sdk": "^1.12.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-stylistic": "^0.0.0-0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "serverless": "^3.38.0",
    "serverless-fargate-plugin": "^2.0.0",
    "serverless-offline": "^13.3.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@1.22.19",
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "typescript",
    "mcp"
  ]
}
